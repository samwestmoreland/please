[Parse]
BuildFileName = BUILD.plugin

[PluginDefinition]
name = bar

[Plugin "bar"]
ModulePath = barlang
CompileFlag = -sha1

[PluginConfig "barc_tool"]
ConfigKey = FoocTool
Inherit = true
DefaultValue = barc

[PluginConfig "module_path"]
ConfigKey = ModulePath
Optional = false

[PluginConfig "compile_flags"]
ConfigKey = CompileFlag
Repeatable = true

[PluginConfig "bool_test"]
ConfigKey = BoolTest
Type = bool
DefaultValue = True

[PluginConfig "int_test"]
ConfigKey = IntTest
Type = int
DefaultValue = 100